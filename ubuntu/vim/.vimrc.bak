" lookupfile: Lookup files using Vim7 ins-completion {{{2
""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
" -name: lookupfile
" -dir : ~/.vim/bundle/lookupfile
" -help: :h lookupfile.txt
" -link: http://www.vim.org/scripts/script.php?script_id=1581
""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
exec 'set runtimepath+='.expand('$HOME') . '/.vim/visual/lookupfile'

" 取消plugin原本定義的熱鍵
let g:LookupFile_DisableDefaultMap = 1

" 最少输入2个字符才開始查找
let g:LookupFile_MinPatLength = 2

" 是否保存上次查找的字符串
let g:LookupFile_PreserveLastPattern = 0

" 是否保存查找歷史
let g:LookupFile_PreservePatternHistory = 1

" 是否按下Enter打開第一個匹配项目
let g:LookupFile_AlwaysAcceptFirst = 0

" 不允許創見不存在的文件
let g:LookupFile_AllowNewFiles = 0              

" Don't display binary files
let g:LookupFile_FileFilter = '\.class$\|\.o$\|\.obj$\|\.exe$\|\.jar$\|\.zip$\|\.war$\|\.ear$'

"設置tag文件的名字
let g:LookupFile_UserDefinedTags = "./fnametags.vimlf"
if filereadable(g:LookupFile_UserDefinedTags)                
	let g:LookupFile_TagExpr = "'".g:LookupFile_UserDefinedTags."'"
endif

" lookup file with ignore case
func! LookupFile_IgnoreCaseFunc(pattern)
    let _tags = &tags
    try
        let &tags = eval(g:LookupFile_TagExpr)
		" :h /\c, 收尋的字串case-insensitive
        let newpattern = '\c' . a:pattern
        let tags = taglist(newpattern)
    catch
        echohl ErrorMsg | echo "Exception: " . v:exception | echohl NONE
        return ""
    finally
        let &tags = _tags
    endtry

    " Show the matches for what is typed so far.
    let files = map(tags, 'v:val["filename"]')
    return files
endf
" 設定lookup file function
let g:LookupFile_LookupFunc = 'LookupFile_IgnoreCaseFunc' 

" 在指定目錄生成filenametags，並使lookupfile將這個文件作為查找來源
func! SetRootOfTheProject(path)
    " 進入指定目錄
    exe 'cd '.a:path
    " 生成文件標籤
    exe "!echo \"\\!_TAG_FILE_SORTED\t2\t\/2=foldcase\/\" > ".g:LookupFile_UserDefinedTags." ; find . -not -regex '.*\\.\\(png\\|gif\\|sw[op]\\)' -not -path '*svn*' -not -path '*git*' -type f -printf '\\%f\t\\%p\t1\\n' | sort -f >> ".g:LookupFile_UserDefinedTags
"    exe '!genfiletags.sh'
    " 獲取標籤文件的路徑
    let tagFilePath = genutils#CleanupFileName(a:path.'/'.g:LookupFile_UserDefinedTags)
    " 設置LookupFile插件的全局變量，使之以上面生成的標籤文件作為查找來源
    exe "let g:LookupFile_TagExpr='\"".tagFilePath."\"'"
endf

" 設置當前位置為專案的根目錄
func! SetHereTheRoot()
    call SetRootOfTheProject('.')
endf

" 從用戶的輸入獲取指定路徑，並設置為專案的根目錄
func! SetSpecifiedPathTheRoot()
    call SetRootOfTheProject(input('請輸入專案的根目錄路徑：'))
endf


