    call MapKeyClear('cscope_maps.vim', 'n', '<C-\>i', '')
    call MapKeyClear('cscope_maps.vim', 'n', '<C-\>d', '')

    call MapKeyClear('cscope_maps.vim', 'n', '<C-@>s', '')
    call MapKeyClear('cscope_maps.vim', 'n', '<C-@>g', '')
    call MapKeyClear('cscope_maps.vim', 'n', '<C-@>c', '')
    call MapKeyClear('cscope_maps.vim', 'n', '<C-@>t', '')
