func! Zod_AndroidCscopeOut()
    !find ./ -name "*.aidl" -o -name "*.cc" -o -name "*.h" -o -name "*.c" -o -name "*.cpp" -o -name "*.java" -o -name "*.mk" > cscope.out
    "!find /usr/include -name "*.h" >> cscope.out
    "-k: don't search /usr/include 
    !cscope -Rkbq -i cscope.out
    !ctags -R --exclude=.svn
    cs add cscope.out
endf

func! Zod_Plugin_cscope_Clear()
    if exists("g:loaded_Zod_Plugin_cscope_Clear")
        return
    endif
    let g:loaded_Zod_Plugin_cscope_Clear = 1

    call MapKeyClear('cscope_maps.vim', 'n', '<C-\>s', '')
