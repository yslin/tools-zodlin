    let g:loaded_Zod_Plugin_cscope = 1

    call Zod_Key_Mapping(1, 0, '', 'cscope_maps.vim', 'nmap', '<unique>', '<C-\>\', ':call Zod_AndroidCscopeOut()<CR>', 'generate cscope.out')

